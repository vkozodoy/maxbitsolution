{
    "extends": [
      "eslint:recommended",
      "plugin:vue/vue3-recommended",
      "@vue/typescript/recommended",
      "prettier",
      "airbnb-base"
    ],
    "env": {
      "browser": true,
      "node": true,
      "es2021": true
    },
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module",
      "parser": "@typescript-eslint/parser"
    },
    "plugins": [
      "vue",
      "@typescript-eslint",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": ["error"],
      "vue/multi-word-component-names": ["error", {
        "ignores": ["index"]
      }]
    }
  }
  